{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "elasticquent/elasticquent": "dev-master",
        "fideloper/proxy": "^4.0",
        "floatingpoint/stylist": "^1.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nette/utils": "^3.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.8",
        "srmklive/paypal": "~1.0",
        "yajra/laravel-datatables-buttons": "^4.5",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Core\\Base\\": "core/base/src",
            "Core\\Slug\\": "core/slug/src",
            "Core\\Media\\": "core/media/src",
            "Core\\Master\\": "core/master/src",
            "Core\\Setting\\": "core/setting/src",
            "Core\\Theme\\": "core/theme/src",
            "Core\\User\\": "core/user/src",
            "Core\\Dashboard\\": "core/dashboard/src",
            "Plugins\\Newsletter\\": "plugins/newsletter/src",
            "Plugins\\Faq\\": "plugins/faq/src",
            "Plugins\\Customer\\": "plugins/customer/src",
            "Plugins\\Product\\": "plugins/product/src",
            "Plugins\\Blog\\": "plugins/blog/src",
            "Plugins\\Addresscode\\": "plugins/addresscode/src",
            "Plugins\\Address\\": "plugins/address/src",
            "Plugins\\Order\\": "plugins/order/src",
            "Plugins\\CustomAttributes\\": "plugins/custom-attributes/src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "psr-4-core": {
        "Core\\Base\\": "core/base/src",
        "Core\\Slug\\": "core/slug/src",
        "Core\\Master\\": "core/master/src",
        "Core\\Setting\\": "core/setting/src",
        "Core\\Theme\\": "core/theme/src",
        "Core\\User\\": "core/user/src",
        "Core\\Dashboard\\": "core/dashboard/src",
        "Core\\Media\\": "core/media/src"
    }
}